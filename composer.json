{
    "name": "craftphp/mini-skeleton",
    "description": "This is a mini skeleton of Craft framework.",
    "require": {
        "php": ">=7.1",
        "datahihi1/tiny-env": "1.0.11",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Craft\\": "src/"
        },
        "files": [
            "src/Application/helper/helper.php",
            "src/Application/helper/source.php",
            "src/Application/helper/deprecation.php",
            "src/Application/helper/TokenGenerator.php",
            "src/Kit/dump/var_dump.php"
        ]
    }
}